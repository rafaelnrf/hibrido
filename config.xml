<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "br.nrfinformaticasolucoes"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>NRF</name>

    <description>
        Descomplicando o Desenvolvimento de Software	
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Rafael Nonato Ribeiro Ferreira
    </author>

<gap:platform name="ios" />
<gap:platform name="android" />
<gap:platform name="winphone" />
<icon src="cbf.png"   gap:platform="ios" width="57" height="57"/>
<icon src="cbf.png"   gap:platform="ios" width="72" height="72"/>
<icon src="cbf.png"   gap:platform="ios" width="120" height="120"/>
<icon src="cbf.png"   gap:platform="ios" width="152" height="152"/>
<icon src="cbf.png"   gap:platform="ios" width="76" height="76"/>
<!-- Android -->
<gap:splash src="splash.png" />
<gap:splash src="cbf.png" gap:platform="android" gap:qualifier="ldpi" />
<gap:splash src="cbf.png" gap:platform="android" gap:qualifier="mdpi" />
<gap:splash src="cbf.png" gap:platform="android" gap:qualifier="hdpi" />
<!-- iPhone 6 -->
<gap:splash src="splash.png" gap:platform="ios" width="320" height="480" />
<gap:splash src="splash.png" gap:platform="ios" width="640" height="960" />
<gap:splash src="splash.png" gap:platform="ios" width="640" height="1136" />
<!-- iPhone 6 -->

<!-- winPhone -->
<gap:splash src="splash.png" gap:platform="winphone" />


<gap:plugin name="org.apache.cordova.battery-status" />
<gap:plugin name="org.apache.cordova.camera" />
<gap:plugin name="org.apache.cordova.media-capture" />
<gap:plugin name="org.apache.cordova.console" />
<gap:plugin name="org.apache.cordova.contacts" />
<gap:plugin name="org.apache.cordova.device" />
<gap:plugin name="org.apache.cordova.device-motion" />
<gap:plugin name="org.apache.cordova.device-orientation" />
<gap:plugin name="org.apache.cordova.dialogs" />
<gap:plugin name="org.apache.cordova.file" />
<gap:plugin name="org.apache.cordova.file-transfer" />
<gap:plugin name="org.apache.cordova.geolocation" />
<gap:plugin name="org.apache.cordova.globalization" />
<gap:plugin name="org.apache.cordova.inappbrowser" />
<gap:plugin name="org.apache.cordova.media" />
<gap:plugin name="org.apache.cordova.network-information" />
<gap:plugin name="org.apache.cordova.splashscreen" />
<gap:plugin name="org.apache.cordova.vibration" />
<gap:plugin name="onesignal-cordova-plugin-pgb-compat" source="npm" />
<gap:plugin name="cordova-plugin-googleplayservices" source="npm" />
<gap:plugin name="cordova-plugin-android-support-v4" source="npm" />
<gap:plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" version="2.1.0" />
    

<access origin="*" />	
<script src="cordova.js"></script>
</widget>